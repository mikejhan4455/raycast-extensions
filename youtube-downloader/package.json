{
   "$schema":"https://www.raycast.com/schemas/extension.json",
   "name":"youtube-downloader",
   "title":"Youtube Downloader",
   "description":"User yt-dlp to download videos and audios",
   "icon":"command-icon.png",
   "author":"mikejhan4455",
   "categories":[
      "Productivity"
   ],
   "license":"MIT",
   "commands":[
      {
         "name":"video-download",
         "title":"Download video",
         "description": "Donwload video from url",
         "mode":"no-view",
         "arguments":[{
               "name":"url",
               "placeholder":"url",
               "type":"text",
               "required":true
            }
         ]
      },
      {
         "name":"audio-download",
         "title":"Download audio",
         "description": "Donwload audio from url",
         "mode":"no-view",
         "arguments":[{
               "name":"url",
               "placeholder":"url",
               "type":"text",
               "required":true
            }
         ]
      }
   ],
   "dependencies":{
      "@raycast/api":"^1.45.2"
   },
   "devDependencies":{
      "@types/node":"18.8.3",
      "@types/react":"18.0.9",
      "@typescript-eslint/eslint-plugin":"^5.0.0",
      "@typescript-eslint/parser":"^5.0.0",
      "eslint":"^7.32.0",
      "eslint-config-prettier":"^8.3.0",
      "prettier":"^2.5.1",
      "typescript":"^4.4.3"
   },
   "scripts":{
      "build":"ray build -e dist",
      "dev":"ray develop",
      "fix-lint":"ray lint --fix",
      "lint":"ray lint",
      "publish":"ray publish"
   }
}